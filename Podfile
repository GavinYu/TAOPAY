target "TAOPAY" do 
platform :ios, ‘10.0’

#加载GIF图片
pod 'SDWebImage' , '~> 4.4.2'
#网络请求
pod 'AFNetworking'
#适配
pod 'Masonry'
#加载提示
pod 'SVProgressHUD'
#强大的YYKit
pod 'YYKit'
#Facebook 的 KVO
pod 'KVOController'
#下拉刷新
pod 'MJRefresh'
#检测内存泄露
pod 'MLeaksFinder'
#滑动手势返回
pod 'FDFullscreenPopGesture'
#底部导航栏
pod 'RDVTabBarController'
#响应式框架
#pod 'ReactiveCocoa'
pod 'ReactiveCocoa', :git => 'https://github.com/zhao0/ReactiveCocoa.git', :tag => '2.5.2'
#路由跳转 JLRoutes、 routable-ios、 HHRouter、 MGJRouter
pod 'MGJRouter'
# 钥匙串存储
pod 'SAMKeychain'
# 加密
pod 'CocoaSecurity'
# UI block
pod 'BlocksKit'
# 轮播图
pod 'SDCycleScrollView'

end
